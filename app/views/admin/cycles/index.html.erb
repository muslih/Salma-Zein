<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="header">
                <h4 class="title">
                Master Cycle
                <%= link_to 'Tambah Cycle', new_admin_cycle_path, :class=>"btn btn-warning btn-fill pull-right" %>
                  
                </h4>
            </div>
            <div class="content table-responsive table-full-width">
                <table class="table table-hover table-striped">
                <thead>
                  <tr>
                    <th>Name</th>
                    <th colspan="3"></th>
                  </tr>
                </thead>

                <tbody>
                  <% @cycles.each do |cycle| %>
                    <tr>
                      <td><%= cycle.name %></td>
                      <td><%= link_to 'Detail', admin_cycle_path(cycle) %></td>
                      <td><%= link_to 'Edit', edit_admin_cycle_path(cycle) %></td>
                      <td><%= link_to 'Hapus', admin_cycle_path(cycle), method: :delete, data: { confirm: 'Yajin Hapus data?' } %></td>
                    </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
        </div>
    </div>
</div>