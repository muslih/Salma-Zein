<%= form_for [:admin, @fund_administration] do |f| %>
  <fieldset>
    <div class="form-group">
      <%= f.label :purchase_order_id, "PO",class:"col-sm-2 control-label" %>
      <div class="col-sm-10">
        <%= f.text_field :purchase_order_id,class:'form-control' %>
      </div>
    </div>
  </fieldset>

  <fieldset>
    <div class="form-group">
      <%= f.label :ammount, "Total",class:"col-sm-2 control-label" %>
      <div class="col-sm-10">
        <%= f.number_field :ammount, class:'form-control' %>
      </div>
    </div>
  </fieldset>

  <fieldset>
    <div class="form-group">
      <%= f.label :date_out, "Tanggal Keluar  ",class:"col-sm-2 control-label" %>
      <div class="col-sm-3">
        <%= f.text_field :date_out,class:'form-control',placeholder:"Tempat lahir" %>
      </div>
      <div class="col-sm-7">
        <%= f.text_field :date_out,'data-value'=>@fund_administration.date_out,'data-behaviour' => 'pickadate',class:'form-control', paceholder: 'Pick a date' %>
      </div>
    </div>
  </fieldset>

  <fieldset>
    <div class="col-sm-2">
    </div>
      <div class="col-sm-10">
        <%= f.submit "Simpan", class:"btn btn-fill btn-primary" %>
      </div>
  </fieldset>
<% end %>
